{
  "name": "@nodenet/core",
  "version": "1.0.3",
  "description": "NodeNet Core Library",
  "main": "test/startup.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src --out-dir dist --extensions \".js\" --source-maps inline"
  },
  "keywords": [
    "NodeNet",
    "NodeNetCore",
    "Core"
  ],
  "author": "Mario Bertsch",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@types/node": "^18.14.1",
    "babel-cli": "^6.26.0"
  },
  "devDependencies": {
    "prettier": "^2.8.4"
  }
}
